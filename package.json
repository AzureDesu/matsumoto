{
  "name": "@azuredesu/matsumoto",
  "private": false,
  "version": "1.4.0",
  "description": "A Vue 3 composable to parse, sanitize, and format Markdown with spoiler support.",
  "type": "module",
  "main": "./dist/matsumoto.umd.cjs",
  "module": "./dist/matsumoto.js",
    "types": "./dist/index.d.ts",
    "exports": {
      ".": {
        "import": {
          "types": "./dist/index.d.ts",
          "default": "./dist/matsumoto.js"
        },
        "require": {
          "types": "./dist/index.d.ts",
          "default": "./dist/matsumoto.umd.cjs"
        }
      },
      "./matsumoto.css": "./dist/matsumoto.css"
    },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "vite build"
  },
  "dependencies": {
    "dompurify": "^3.2.6"
  },
  "devDependencies": {
    "@types/dompurify": "^3.0.5",
    "@types/node": "^24.2.0",
    "typescript": "^5.9.2",
    "vite": "^7.1.1",
    "vite-plugin-dts": "^4.5.4",
    "vue": "^3.5.18"
  },
  "peerDependencies": {
    "vue": "^3.5.18"
  },
  "keywords": [
    "vue",
    "composable",
    "parser",
    "matsumoto"
  ],
  "author": "AzureDesu <AzureDesu@protonmail.com>",
  "license": "GPL-3.0-or-later",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AzureDesu/matsumoto.git"
  }
}
